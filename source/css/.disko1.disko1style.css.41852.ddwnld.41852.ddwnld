}

.header__nav-toggle {
  display: block;
  box-sizing: border-box;
  width: 23px;
  min-height: 22px;
  background-image: url(../img/header/icon-menu-toogle.svg);
  background-repeat: no-repeat;
  padding: 0;
  border: none;
  font: inherit;
