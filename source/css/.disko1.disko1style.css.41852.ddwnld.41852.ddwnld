}

.container-center {
  width: 280px;
  margin: 0 auto;
}

.page-body {
  margin: 0;
}

.page-wrapper {
  display: grid;
  grid-template-rows: -webkit-min-content 1fr -webkit-min-content;
  grid-template-rows: min-content 1fr min-content;
  align-content: start;
  min-height: 100vh;
  background-color: #ffffff;
  width: 320px;
  margin: 0 auto;
  box-shadow: 0 5px 15px 0 rgba(0, 0, 0, 0.2);
  box-sizing: border-box;
}

@media (min-width: 768px) {
  .page-wrapper {
    width: 768px;
  }
}
.header {
  position: relative;
}

.header__nav-wrapper {
  min-height: 56px;
  background-color: #ffffff;
}

.header__nav {
