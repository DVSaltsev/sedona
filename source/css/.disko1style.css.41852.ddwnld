}

.page-body {
  margin: 0;
}

.page-wrapper {
  display: grid;
  grid-template-rows: -webkit-min-content 1fr -webkit-min-content;
  grid-template-rows: min-content 1fr min-content;
  align-content: start;
  min-height: 100vh;
  background-color: #ffffff;
  width: 320px;
  margin: 0 auto;
  box-shadow: 0 5px 15px 0 rgba(0, 0, 0, 0.2);
  box-sizing: border-box;
}

@media (min-width: 768px) {
  .page-wrapper {
    width: 768px;
  }
}
@media (min-width: 1200px) {
  .page-wrapper {
    width: 1200px;
  }
}
.header {
  position: relative;
}

.header__nav-wrapper {
  min-height: 56px;
  background-color: #ffffff;
  display: flex;
  justify-content: right;
  margin: 0 20px;
}

.header__nav {
  display: none;
}

.header__logo-link {
  position: absolute;
  left: 50%;
  transform: translate(-50%);
}

.header__nav-toggle {
  display: block;
  box-sizing: border-box;
  width: 23px;
  height: 22px;
  background-image: url(../img/header/icon-menu-toogle.svg);
  background-repeat: no-repeat;
  padding: 0;
  border: none;
  font: inherit;
  color: inherit;
  cursor: pointer;
}

.header__nav-link {
  display: flex;
  box-sizing: border-box;
  text-decoration: none;
  padding: 10px;
  background: #81B3D2;
  mix-blend-mode: normal;
  align-items: center;
  justify-content: center;
  cursor: pointer;
}

.header__main-banner {
  background-image: url(../img/header/back-desktop.jpg);
  background-repeat: no-repeat;
  background-size: cover;
  min-height: 157px;
}

.header__main-banner-form {
  background-image: url(../img/header/header-back-mobile.jpg);
  background-repeat: no-repeat;
  background-size: cover;
  min-height: 157px;
}

